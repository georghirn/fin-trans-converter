{
   "version": "0.2.0",
   "configurations": [
      {
         "name": ".NET Core Launch (console)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceRoot}/src/finTransConverter/bin/Debug/netcoreapp1.0/FinTransConverter.dll",
         "args": [
             "/home/georg/Dokumente/Finanzen/Kontoauszuege/HelloBank/Kreditkarte/2016/HelloBank-kreditkarte_2016-09_bis_2016-09-18.html", 
             "/home/georg/Dokumente/Finanzen/Homebank/test/georg-hirn-finanzen-ab-2016-01-01.xhb", 
             "HelloBankToHomebank",
             "--verbose",
             "--account-type=CreditCardAccount",
             "--append-duplicates", 
             "--check-only",
             //"--homebank-settings-file=/home/georg/Dokumente/Finanzen/Homebank/GeorgHirn_Finanzen.xhb",
             "--paymode-patterns-file=/home/georg/Dokumente/Finanzen/Homebank/settings/hellobank_kreditkarte_paymode_patterns.xpmp", 
             "--transaction-assignments-file=/home/georg/Dokumente/Finanzen/Homebank/settings/hellobank_transaction_assignments.xtasg", 
             "--target-account-pattern=\".*Kreditkarte.*Hello.*Bank.*\""
         ],
         "cwd": "${workspaceRoot}/src/finTransConverter",
         "stopAtEntry": false,
         "externalConsole": false
      },
      {
         "name": ".NET Core Launch (web)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceRoot}/src/finTransConverter/bin/Debug/netcoreapp1.0/FinTransConverter.dll",
         "args": [],
         "cwd": "${workspaceRoot}/src/finTransConverter",
         "stopAtEntry": false,
         "launchBrowser": {
            "enabled": true,
            "args": "${auto-detect-url}",
            "windows": {
               "command": "cmd.exe",
               "args": "/C start ${auto-detect-url}"
            },
            "osx": {
               "command": "open"
            },
            "linux": {
               "command": "xdg-open"
            }
         },
         "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
         },
         "sourceFileMap": {
            "/Views": "${workspaceRoot}/Views"
         }
      },
      {
         "name": ".NET Core Attach",
         "type": "coreclr",
         "request": "attach",
         "processId": "${command.pickProcess}"
      }
   ]
}