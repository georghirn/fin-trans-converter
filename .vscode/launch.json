{
   "version": "0.2.0",
   "configurations": [
      {
         "name": ".NET Core Launch (console)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceRoot}/src/finTransConverter/bin/Debug/netcoreapp1.0/finTransConverter.dll",
         "args": [
             "/home/georg/Dokumente/Finanzen/Kontoauszuege/HelloBank/umsaetze-girokonto_19250_65303084205_EUR_2016-08-20_17-33-14.csv", 
             "outputfile", 
             "HelloBankToHomebank", 
             "--homebankSettingsFile=/home/georg/Dokumente/Finanzen/HomeBank/GeorgHirn_Finanzen.xhb",
             "--paymodePatternsFile=/home/georg/Dokumente/Finanzen/HomeBank/hellobank_girokonto_paymode_patterns.xpmp"
         ],
         "cwd": "${workspaceRoot}/src/finTransConverter",
         "stopAtEntry": false,
         "externalConsole": false
      },
      {
         "name": ".NET Core Launch (web)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceRoot}/src/finTransConverter/bin/Debug/netcoreapp1.0/finTransConverter.dll",
         "args": [],
         "cwd": "${workspaceRoot}/src/finTransConverter",
         "stopAtEntry": false,
         "launchBrowser": {
            "enabled": true,
            "args": "${auto-detect-url}",
            "windows": {
               "command": "cmd.exe",
               "args": "/C start ${auto-detect-url}"
            },
            "osx": {
               "command": "open"
            },
            "linux": {
               "command": "xdg-open"
            }
         },
         "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
         },
         "sourceFileMap": {
            "/Views": "${workspaceRoot}/Views"
         }
      },
      {
         "name": ".NET Core Attach",
         "type": "coreclr",
         "request": "attach",
         "processId": "${command.pickProcess}"
      }
   ]
}